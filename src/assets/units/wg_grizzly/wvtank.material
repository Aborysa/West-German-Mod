import * from "BZBase.material"

material wvtank00 : BZBase
{
	set_texture_alias DiffuseMap wvtank_D.dds
	set_texture_alias NormalMap wvtank_N.dds
	set_texture_alias SpecularMap wvtank_S.dds
	set_texture_alias EmissiveMap wvtank_E.dds
	
	set $diffuse "1 1 1"
	set $ambient "1 1 1"
	set $specular ".7 .7 .7"
	set $shininess "127"
	set $glow "1 1 1"
}

